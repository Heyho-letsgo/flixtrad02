
<header id="content-header">
  <h1><%= pluralize(@users.size, 'User') %></h1>
</header>


<ul id="users">
  <% @users.each do |user| %>
      <li>
        <%= profile_image_for_index(user) %>
        <%=	 user.id %> |
        <%= link_to user.name, user %> |
        <%= user.created_at.strftime('Membre depuis le: %A/%d/%m/%Y à  %H:%M') %>
             </li>
  <% end %>
</ul>


<%= link_to"ordonné", users_path%>